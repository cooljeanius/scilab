            # remove that part
        elif test -x "`which port`"; then
        	if test -L "`which port`"; then
        	    REAL_PORT=$(readlink `which port`)
        	    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: \`which port\` is a symlink to ${REAL_PORT}." >&5
